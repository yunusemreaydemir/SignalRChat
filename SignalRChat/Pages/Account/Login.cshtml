@page
@model ChatWithSignalR.Pages.Account.LoginModel
@{
}

<form asp-page="/Account/Login">

    <div class="form-group">
        <label>Kullanıcı Adı</label>
        <input asp-for="UserModel.UserName" class="form-control" />
    </div>

    <div class="form-group">
        <label>Şifre</label>
        <input type="password" asp-for="UserModel.Password" class="form-control" />
    </div>

    <button class="btn btn-info" type="submit">Giriş Yap</button>

</form>

